<core:FragmentDefinition xmlns="sap.m"
xmlns:core="sap.ui.core">                
        <Dialog id='idDialog'
                title='{i18n>itemSeleccionado}' resizable="true">
                    <VBox >
                    <Text text="Producto :   {modeloDialog>/Producto} "></Text>
                    <Text text="Proveedor : {modeloDialog>/Proveedor} "></Text>
                    <Text text="Tamaño : {modeloDialog>/Tamaño} "></Text>
                    <Text text="Peso : {modeloDialog>/Peso} "></Text>
                    </VBox>
                    <beginButton>
                        <Button text="Cerrar" press=".onCloseDialog"></Button>
                    </beginButton>
        </Dialog>
</core:FragmentDefinition>